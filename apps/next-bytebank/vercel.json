{
  "rewrites": [
    {
      "source": "/auth",
      "destination": "https://fiap-bytebank-auth.vercel.app/auth"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://fiap-bytebank-auth.vercel.app/auth/:path*"
    },

    {
      "source": "/nest/:path*",
      "destination": "https://nest-bytebank-api.vercel.app/:path*"
    }
  ]
}
